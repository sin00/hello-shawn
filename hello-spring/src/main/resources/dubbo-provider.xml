<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
						http://code.alibabatech.com/schema/dubbo 
						http://code.alibabatech.com/schema/dubbo/dubbo.xsd
						http://www.springframework.org/schema/context
						http://www.springframework.org/schema/context/spring-context-4.0.xsd">
	<dubbo:application name="hellodubbo" owner="hello" organization="ericsson" />
    <dubbo:registry address="zookeeper://172.21.38.19:2181"/>
<!-- 	<dubbo:protocol name="rest" port="${dubbo.uploader.port}" contextpath="uploader-backend/services" server="servlet"/> -->
	<dubbo:protocol name="dubbo" port="20880" />
	<bean id="helloService" class="com.ericsson.li.dubbo.provider.ProvideServiceImpl" />
	<dubbo:service interface="com.ericsson.li.dubbo.provider.ProvideService" ref="helloService" validation="false" protocol="rest"/>
</beans>